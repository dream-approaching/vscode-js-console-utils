{
  "name": "console-utils-fork",
  "displayName": "console log quickly",
  "description": "Help insert and remove console.(*) statements",
  "version": "0.1.1",
  "publisher": "longzi",
  "repository": "https://github.com/dream-approaching/vscode-js-console-utils",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": ["Other"],
  "icon": "images/log.png",
  "activationEvents": [
    "onCommand:extension.insertLogStatement",
    "onCommand:extension.deleteAllLogStatements"
  ],
  "main": "./extension",
  "contributes": {
		"configuration": {
      "type": "object",
      "title": "Console utils",
      "properties": {
          "consoleUtils.namePrefix": {
              "type": "string",
              "default": " ",
              "description": "名字前缀"
          },
          "consoleUtils.color": {
              "type": "string",
              "default": "#0e93e0",
              "description": "字体颜色"
          },
          "consoleUtils.colorBg": {
              "type": "string",
              "default": "#aaefe5",
              "description": "背景颜色"
          },
          "consoleUtils.showSemi": {
              "type": "boolean",
              "default": true,
              "description": "末尾是否加分号"
          }
      }
  },
    "commands": [
      {
        "command": "extension.insertLogStatement",
        "title": "Insert Log Statement"
      },
      {
        "command": "extension.deleteAllLogStatements",
        "title": "Delete all console.log statements"
      }
    ],
    "keybindings": [
      {
        "command": "extension.insertLogStatement",
        "key": "shift+ctrl+l",
        "mac": "shift+cmd+l",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.deleteAllLogStatements",
        "key": "shift+ctrl+d",
        "mac": "shift+cmd+d"
      }
    ],
    "menus": {
      "editor/context": [
          {
              "when": "editorFocus",
              "command": "extension.insertLogStatement",
              "group": "navigation"
          }
      ]
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "eslint": "^3.6.0",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}
